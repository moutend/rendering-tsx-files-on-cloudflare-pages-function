{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2022",
    "lib": [
      "es2021"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "nano-jsx/lib",
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ]
    }
  }
}
